<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admn.rxjava2demo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.SampleDemoActivity" />
        <activity android:name=".Activity.GetEdittextStringActivity" />
        <activity android:name=".Activity.EditTextChangeActivity" />
        <activity android:name=".Activity.ArrayListActivity" />
        <activity android:name=".Activity.ObservableTypesActivity" />
        <activity android:name=".Activity.SingleObservableActivity" />
        <activity android:name=".Activity.MayBeObservableActivity" />
        <activity android:name=".Activity.CompletableObservableActivity" />
        <activity android:name=".Activity.FlowableObservableActivity" />
        <activity android:name=".Activity.SearchLocalActivity" />
        <activity android:name=".Activity.ApiCallActivity"></activity>
    </application>

</manifest>